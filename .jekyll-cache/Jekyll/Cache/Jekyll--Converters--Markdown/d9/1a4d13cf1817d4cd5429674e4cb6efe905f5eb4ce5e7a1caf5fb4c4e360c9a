	<span class="p">}</span>

	<span class="n">s</span><span class="o">.</span><span class="n">m</span><span class="p">[</span><span class="n">i</span><span class="p">]</span> <span class="o">=</span> <span class="m">1</span>

	<span class="k">return</span> <span class="no">true</span>
<span class="p">}</span>

