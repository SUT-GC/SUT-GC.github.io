	<span class="n">result</span> <span class="o">:=</span> <span class="s">""</span>

	<span class="n">result</span> <span class="o">+=</span> <span class="n">numPool</span><span class="p">[</span><span class="n">num</span><span class="o">/</span><span class="m">1000</span><span class="o">*</span><span class="m">1000</span><span class="p">]</span>
	<span class="n">num</span> <span class="o">=</span> <span class="n">num</span> <span class="o">%</span> <span class="m">1000</span>
	<span class="n">result</span> <span class="o">+=</span> <span class="n">numPool</span><span class="p">[</span><span class="n">num</span><span class="o">/</span><span class="m">100</span><span class="o">*</span><span class="m">100</span><span class="p">]</span>
	<span class="n">num</span> <span class="o">=</span> <span class="n">num</span> <span class="o">%</span> <span class="m">100</span>
	<span class="n">result</span> <span class="o">+=</span> <span class="n">numPool</span><span class="p">[</span><span class="n">num</span><span class="o">/</span><span class="m">10</span><span class="o">*</span><span class="m">10</span><span class="p">]</span>
	<span class="n">num</span> <span class="o">=</span> <span class="n">num</span> <span class="o">%</span> <span class="m">10</span>
	<span class="n">result</span> <span class="o">+=</span> <span class="n">numPool</span><span class="p">[</span><span class="n">num</span><span class="p">]</span>

	<span class="k">return</span> <span class="n">result</span>
<span class="p">}</span>

