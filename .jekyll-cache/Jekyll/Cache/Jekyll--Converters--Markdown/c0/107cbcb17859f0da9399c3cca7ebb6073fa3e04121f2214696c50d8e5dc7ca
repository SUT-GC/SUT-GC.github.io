    <span class="kd">public</span> <span class="kd">static</span> <span class="nc">Node</span> <span class="nf">mergeTwoList</span><span class="o">(</span><span class="nc">Node</span> <span class="n">root1</span><span class="o">,</span> <span class="nc">Node</span> <span class="n">root2</span><span class="o">){</span>
        <span class="nc">Node</span> <span class="n">head</span> <span class="o">=</span> <span class="kc">null</span><span class="o">;</span>
        <span class="k">if</span><span class="o">(</span><span class="n">root1</span> <span class="o">==</span> <span class="kc">null</span> <span class="o">&amp;&amp;</span> <span class="n">root2</span> <span class="o">==</span> <span class="kc">null</span><span class="o">)</span>
            <span class="k">return</span> <span class="kc">null</span><span class="o">;</span>
        <span class="k">if</span><span class="o">(</span><span class="n">root1</span> <span class="o">==</span> <span class="kc">null</span> <span class="o">)</span>
            <span class="k">return</span> <span class="n">root2</span><span class="o">;</span>
        <span class="k">if</span><span class="o">(</span><span class="n">root2</span> <span class="o">==</span> <span class="kc">null</span><span class="o">)</span>
            <span class="k">return</span> <span class="n">root1</span><span class="o">;</span>
        <span class="k">if</span><span class="o">(</span><span class="n">root1</span><span class="o">.</span><span class="na">getValue</span><span class="o">()</span> <span class="o">&gt;</span> <span class="n">root2</span><span class="o">.</span><span class="na">getValue</span><span class="o">()){</span>
            <span class="n">head</span> <span class="o">=</span> <span class="n">root2</span><span class="o">;</span>
            <span class="n">head</span><span class="o">.</span><span class="na">next</span> <span class="o">=</span> <span class="n">mergeTwoList</span><span class="o">(</span><span class="n">root1</span><span class="o">,</span> <span class="n">root2</span><span class="o">.</span><span class="na">next</span><span class="o">);</span>
        <span class="o">}</span><span class="k">else</span><span class="o">{</span>
            <span class="n">head</span> <span class="o">=</span> <span class="n">root1</span><span class="o">;</span>
            <span class="n">head</span><span class="o">.</span><span class="na">next</span> <span class="o">=</span> <span class="n">mergeTwoList</span><span class="o">(</span><span class="n">root1</span><span class="o">.</span><span class="na">next</span><span class="o">,</span> <span class="n">root2</span><span class="o">);</span>
        <span class="o">}</span>
        <span class="k">return</span> <span class="n">head</span><span class="o">;</span>
    <span class="o">}</span>
    <span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="nc">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
        <span class="nc">Node</span> <span class="n">root1</span> <span class="o">=</span> <span class="nc">TheKNumInLast</span><span class="o">.</span><span class="na">createList</span><span class="o">(</span><span class="mi">10</span><span class="o">);</span>
        <span class="nc">Node</span> <span class="n">root2</span> <span class="o">=</span> <span class="nc">TheKNumInLast</span><span class="o">.</span><span class="na">createList</span><span class="o">(</span><span class="mi">10</span><span class="o">);</span>
