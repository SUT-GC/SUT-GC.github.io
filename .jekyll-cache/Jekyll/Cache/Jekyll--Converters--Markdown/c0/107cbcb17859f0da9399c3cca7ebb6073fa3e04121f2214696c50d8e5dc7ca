    <span class="o">}</span>
    <span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">printTreeFirstRootFor</span><span class="o">(</span><span class="nc">BinaryTreeNode</span> <span class="n">treeRoot</span><span class="o">){</span>
        <span class="k">if</span><span class="o">(</span><span class="n">treeRoot</span> <span class="o">==</span> <span class="kc">null</span><span class="o">){</span>
            <span class="k">return</span><span class="o">;</span>
        <span class="o">}</span>
        <span class="nc">LinkedList</span><span class="o">&lt;</span><span class="nc">BinaryTreeNode</span><span class="o">&gt;</span> <span class="n">treeNodeStack</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">LinkedList</span><span class="o">&lt;&gt;();</span>
        <span class="n">treeNodeStack</span><span class="o">.</span><span class="na">push</span><span class="o">(</span><span class="n">treeRoot</span><span class="o">);</span>
        <span class="k">while</span><span class="o">(!</span><span class="n">treeNodeStack</span><span class="o">.</span><span class="na">isEmpty</span><span class="o">()){</span>
            <span class="nc">BinaryTreeNode</span> <span class="n">treeNode</span> <span class="o">=</span> <span class="n">treeNodeStack</span><span class="o">.</span><span class="na">getLast</span><span class="o">();</span>
            <span class="n">treeNodeStack</span><span class="o">.</span><span class="na">removeLast</span><span class="o">();</span>
            <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">treeNode</span><span class="o">.</span><span class="na">getValue</span><span class="o">());</span>
            <span class="k">if</span><span class="o">(</span><span class="n">treeNode</span><span class="o">.</span><span class="na">rightNode</span> <span class="o">!=</span> <span class="kc">null</span><span class="o">){</span>
                <span class="n">treeNodeStack</span><span class="o">.</span><span class="na">addLast</span><span class="o">(</span><span class="n">treeNode</span><span class="o">.</span><span class="na">rightNode</span><span class="o">);</span>
            <span class="o">}</span>
            <span class="k">if</span><span class="o">(</span><span class="n">treeNode</span><span class="o">.</span><span class="na">leftNode</span> <span class="o">!=</span> <span class="kc">null</span><span class="o">){</span>
                <span class="n">treeNodeStack</span><span class="o">.</span><span class="na">addLast</span><span class="o">(</span><span class="n">treeNode</span><span class="o">.</span><span class="na">leftNode</span><span class="o">);</span>
            <span class="o">}</span>
            
            
        <span class="o">}</span>
    <span class="o">}</span>
    <span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">printTreeSecoundRootFor</span><span class="o">(</span><span class="nc">BinaryTreeNode</span> <span class="n">treeRoot</span><span class="o">){</span>
        <span class="k">if</span><span class="o">(</span><span class="n">treeRoot</span> <span class="o">==</span> <span class="kc">null</span><span class="o">){</span>
            <span class="k">return</span><span class="o">;</span>
        <span class="o">}</span>
        <span class="nc">LinkedList</span><span class="o">&lt;</span><span class="nc">BinaryTreeNode</span><span class="o">&gt;</span> <span class="n">treeNodeStack</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">LinkedList</span><span class="o">&lt;&gt;();</span>
        <span class="n">treeNodeStack</span><span class="o">.</span><span class="na">addLast</span><span class="o">(</span><span class="n">treeRoot</span><span class="o">);</span>
