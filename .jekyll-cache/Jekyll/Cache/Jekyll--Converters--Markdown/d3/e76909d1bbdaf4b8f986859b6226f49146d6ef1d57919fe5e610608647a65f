            <span class="nx">deleteRomanIndex</span> <span class="o">=</span> <span class="nx">key</span><span class="p">.</span><span class="nx">length</span>
        <span class="p">}</span>
    <span class="p">}</span>

    <span class="nx">resultNumber</span> <span class="o">+=</span> <span class="nx">addNumber</span>
    <span class="nx">s</span> <span class="o">=</span> <span class="nx">s</span><span class="p">.</span><span class="nx">substr</span><span class="p">(</span><span class="nx">deleteRomanIndex</span><span class="p">)</span>
             

    <span class="kd">var</span> <span class="nx">addNumber</span> <span class="o">=</span> <span class="mi">0</span>
    <span class="k">for</span><span class="p">(</span><span class="kd">var</span> <span class="nx">key</span> <span class="k">in</span> <span class="nx">singleDigit</span><span class="p">){</span>
        <span class="k">if</span><span class="p">(</span><span class="nx">s</span><span class="p">.</span><span class="nx">startsWith</span><span class="p">(</span><span class="nx">key</span><span class="p">)){</span>
