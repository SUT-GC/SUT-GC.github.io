        <span class="p">}</span>
    <span class="p">}</span>

    <span class="nx">resultNumber</span> <span class="o">+=</span> <span class="nx">addNumber</span>
    <span class="k">return</span> <span class="nx">resultNumber</span> 
      
<span class="p">};</span>

